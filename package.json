{
  "name": "@polar-sh/sdk",
  "version": "0.32.0",
  "author": "Speakeasy",
  "type": "module",
  "bin": {
    "mcp": "bin/mcp-server.js"
  },
  "tshy": {
    "sourceDialects": [
      "@polar-sh/sdk/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/polarsource/polar-js.git"
  },
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "build:mcp": "bun src/mcp-server/build.mts",
    "build": "npm run build:mcp && tshy",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.5.0",
    "zod": ">= 3"
  },
  "peerDependenciesMeta": {
    "@modelcontextprotocol/sdk": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "@stricli/core": "^1.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "*",
    "@types/node": "*",
    "bun": "^1.2.2",
    "bun-types": "^1.2.2",
    "eslint": "^9.19.0",
    "express": "^4.21.2",
    "globals": "^15.14.0",
    "jest": "*",
    "ts-jest": "*",
    "tshy": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.22.0",
    "zod": "^3.23.4"
  },
  "dependencies": {
    "standardwebhooks": "^1.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "@polar-sh/sdk/source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json",
    "./*.js": {
      "import": {
        "@polar-sh/sdk/source": "./src/*.ts",
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    },
    "./*": {
      "import": {
        "@polar-sh/sdk/source": "./src/*.ts",
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
